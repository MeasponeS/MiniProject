.rect-progress{

    border-radius: 4rpx;
    margin-right: 20rpx;
    height: 28rpx;
    width: 46rpx;
    /*background: #969FA9;*/
    overflow: hidden;
    position: relative;
}
.bg-progress{
    position: absolute;
    top: 0;
    left: 0;
    height: 100%;
    z-index: 2;
    /*background: #31B68F;*/
}
.text-progress{
    position: absolute;
    top: 0;
    left: 0;
    z-index: 5;
    width: 100%;
    height: 100%;
    display: inline-block;
    font-size: 18rpx;
    color: #fff;
    text-align: center;
    line-height: 28rpx;
}
