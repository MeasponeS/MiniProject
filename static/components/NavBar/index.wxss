.header {
        position: fixed;
        overflow: hidden;
        top: 0;
        width: 100%;
    }
    .nav-bar{
        display: flex;
        align-items: center;
        color: #fff;
        font-size: 34rpx;
    }
    .nav-left,.nav-right{
        width: 136rpx;
        height: 100%;
        display: flex;
        align-items: center;
    }
    .nav-center{
        height: 100%;
        flex: 1;
        display: flex;
        align-items: center;
        justify-content: center;
    }
    .back-icon{
        width: 20rpx;
        height: 35rpx;
        margin: 0 15rpx 0 20rpx;
    }
